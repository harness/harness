{
  "mainHeader": "Generic Client Setup",
  "secHeader": "Follow these instructions to install/use Generic artifacts or compatible packages.",
  "sections": [
    {
      "header": "Generate identity token",
      "steps": [
        {
          "header": "An identity token will serve as the password for uploading and downloading artifact.",
          "type": "GenerateToken"
        }
      ],
      "type": "INLINE"
    },
    {
      "header": "Upload Artifact",
      "steps": [
        {
          "commands": [
            {
              "label": "",
              "value": "curl --location --request PUT 'http://example.com/registry/test-registry/GENERIC/generic/test-artifact/v1.0.0' \\\n--form 'filename=\"\u003cFILENAME\u003e\"' \\\n--form 'file=@\"\u003cFILE_PATH\u003e\"' \\\n--form 'description=\"\u003cDESC\u003e\"' \\\n--header 'Authorization: Bearer \u003cAPI_KEY\u003e'"
            }
          ],
          "header": "Run this curl command in your terminal to push the artifact at package level. This command works for non-nested paths.",
          "type": "Static"
        }
      ],
      "type": "INLINE"
    },
    {
      "header": "Download Artifact",
      "steps": [
        {
          "commands": [
            {
              "label": "",
              "value": "curl --location 'http://example.com/registry/test-registry/GENERIC/generic/test-artifact/v1.0.0?filename=\u003cFILENAME\u003e' \\\n --header 'Authorization: Bearer \u003cAPI_KEY\u003e' -J -O"
            }
          ],
          "header": "Run this command in your terminal to download the artifact at package level. This command works for non-nested paths.",
          "type": "Static"
        }
      ],
      "type": "INLINE"
    },
    {
      "header": "File Operations (Supports both nested and flat paths)",
      "steps": [
        {
          "commands": [
            {
              "label": "",
              "value": "curl -XPUT 'http://example.com/registry/test-registry/GENERIC/files/test-artifact/v1.0.0/\u003cNESTED_FILE_PATH\u003e' \\\n--header 'Authorization: Bearer \u003cAPI_KEY\u003e' -T '\u003cFILE_PATH\u003e'"
            }
          ],
          "header": "Upload a file to a specific path within the package.",
          "type": "Static"
        },
        {
          "commands": [
            {
              "label": "",
              "value": "curl --location 'http://example.com/registry/test-registry/GENERIC/files/test-artifact/v1.0.0/\u003cNESTED_FILE_PATH\u003e' --header 'Authorization: Bearer \u003cAPI_KEY\u003e' -J -O"
            }
          ],
          "header": "Download a file from a specific path within the package.",
          "type": "Static"
        },
        {
          "commands": [
            {
              "label": "",
              "value": "curl --location --head 'http://example.com/registry/test-registry/GENERIC/files/test-artifact/v1.0.0/\u003cNESTED_FILE_PATH\u003e' --header 'Authorization: Bearer \u003cAPI_KEY\u003e'"
            }
          ],
          "header": "Get file metadata from a specific path (HEAD request).",
          "type": "Static"
        },
        {
          "commands": [
            {
              "label": "",
              "value": "curl --location --request DELETE 'http://example.com/registry/test-registry/GENERIC/files/test-artifact/v1.0.0/\u003cNESTED_FILE_PATH\u003e' --header 'Authorization: Bearer \u003cAPI_KEY\u003e'"
            }
          ],
          "header": "Delete a file from a specific path within the package.",
          "type": "Static"
        }
      ],
      "type": "INLINE"
    }
  ]
}